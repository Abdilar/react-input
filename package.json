{
  "name": "react-input",
  "version": "1.0.0",
  "description": "This is an input component for React",
  "author": "Saeed Abdilar",
  "license": "MIT",
  "repository": "Abdilar/react-input",
  "homepage": "https://abdilar.github.io/react-input",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --css-modules=[local] --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "npm run build-storybook",
    "deploy-storybook": "npm run predeploy && npm run deploy:storybook",
    "deploy:storybook": "gh-pages -d storybook-static",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "sakit-sa",
    "input",
    "react",
    "spinner",
    "xss",
    "password",
    "number comma seprator",
    "react-input",
    "javascritp",
    "component",
    "front-end"
  ],
  "dependencies": {
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-viewport": "^6.2.9",
    "@storybook/builder-webpack5": "^6.2.9",
    "@storybook/preset-scss": "^1.0.3",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^3.4.2",
    "node-sass": "^6.0.0",
    "prop-types": "^15.7.2",
    "sass-loader": "^8.0.2",
    "stream-browserify": "^2.0.2",
    "style-loader": "^0.23.1"
  }
}
